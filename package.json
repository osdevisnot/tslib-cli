{
  "name": "tslib-cli",
  "version": "0.0.27",
  "description": "Maximum Overkill for Typescript Library Authors",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "prepub": "npm run setup",
    "pub": "npm version patch && npm publish",
    "postpub": "git push --follow-tags",
    "presetup": "git clean -fdX",
    "setup": "npm install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true
  },
  "dependencies": {
    "@types/jest": "23.3.10",
    "@types/node": "10.12.10",
    "jest": "23.6.0",
    "rollup": "0.67.3",
    "rollup-plugin-commonjs": "9.2.0",
    "rollup-plugin-filesize": "5.0.1",
    "rollup-plugin-html": "0.2.1",
    "rollup-plugin-json": "3.1.0",
    "rollup-plugin-livereload": "0.6.0",
    "rollup-plugin-minify-html-literals": "1.1.2",
    "rollup-plugin-multi-entry": "2.0.2",
    "rollup-plugin-node-resolve": "3.4.0",
    "rollup-plugin-replace": "2.1.0",
    "rollup-plugin-serve": "0.6.0",
    "rollup-plugin-terser": "4.0.0",
    "rollup-plugin-typescript2": "0.18.0",
    "ts-jest": "23.10.5",
    "tslint": "5.11.0",
    "tslint-config-airbnb": "5.11.1",
    "tslint-config-prettier": "1.17.0",
    "typedoc": "0.13.0",
    "typedoc-plugin-external-module-name": "1.1.3",
    "typedoc-plugin-internal-external": "1.0.10",
    "typedoc-plugin-markdown": "1.1.19",
    "typedoc-plugin-no-inherit": "1.0.2",
    "typescript": "3.1.6"
  },
  "devDependencies": {
    "husky": "1.2.0",
    "lint-staged": "8.1.0",
    "prettier": "1.15.2",
    "sort-package-json": "1.16.0"
  },
  "lint-staged": {
    "*.{js,ts,json,css}": [
      "prettier --write",
      "git add"
    ],
    "*package.json": [
      "sort-package-json",
      "git add"
    ]
  }
}
